<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class dynamic-insert="false" dynamic-update="false" mutable="true" name="School.Model.School_Login" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
        <id name="id" column="global_id" type="integer">
            <generator class="native"></generator>
        </id>
        <property name="email" column="email" type="string"></property>
        <property name="password" column="password" type="string"></property>
    </class>
    

</hibernate-mapping>
